    <servlet-name>JieSuangServlet</servlet-name>
    <servlet-class>cn.fosu.dao.servlet.JieSuangServlet</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/servlet/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAjaxServlet</servlet-name>
    <url-pattern>/servlet/UserAjaxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/servlet/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexSevlet</servlet-name>
    <url-pattern>/servlet/IndexSevlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SP_carServlet</servlet-name>
    <url-pattern>/servlet/SP_carServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SP_carInfoServlet</servlet-name>
    <url-pattern>/servlet/SP_carInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JieSuangServlet</servlet-name>
    <url-pattern>/servlet/JieSuangServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
	 <welcome-file>Welcome.jsp</welcome-file>
  </welcome-file-list>
  

</web-app>