    <servlet-class>cn.fosu.dao.servlet.SP_carServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SP_carInfoServlet</servlet-name>
    <servlet-class>cn.fosu.dao.servlet.SP_carInfoServlet</servlet-class>
  </servlet>
  <servlet>
